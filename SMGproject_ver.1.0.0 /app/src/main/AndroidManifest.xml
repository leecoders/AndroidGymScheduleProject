<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hdh.smgproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fossil_logo"
        android:label="FOSSIL GYM"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".Splashscreen"
            android:label="FOSSIL GYM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".UserMainActivity"
            android:label="FOSSIL GYM"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Pop"
            android:theme="@style/AppTheme.Pop"></activity>
        <activity android:name=".PopTrainerInfo"
            android:theme="@style/AppTheme.Pop"></activity>
        <activity android:name=".PopUserInfo"
        android:theme="@style/AppTheme.Pop"></activity>
        <activity android:name=".PopFeedBack"
            android:theme="@style/AppTheme.Pop"></activity>
        <activity android:name=".PopFeedBackOfUser"
            android:theme="@style/AppTheme.Pop"></activity>
        <activity android:name=".CEOMainActivity" />
        <activity android:name=".TRregisterActivity" />
        <activity android:name=".UserInfoChange" />
        <activity
            android:name=".TrainerMainActivity"
            android:label="FOSSIL GYM"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>